{
  "StressOptions": {
    "DistributedPubSubOptions": {
      "Enabled": true,
      "NumTopics": 100
    },
    "ShardingOptions": {
      "Enabled": true,
      "UseDData": false,
      "RememberEntities": true
    },
    "AkkaClusterOptions": {
      "Hostname": "localhost",
      "Port": 9221,
      "ManagementPort": 9228,
      "UseKubernetesDiscovery": false,
      "UseKubernetesLease": false,
      "KubernetesDiscoveryOptions": {
        "PodNamespace": "akkastress",
        "PodLabelSelector": "akkacluster=stress"
      },
      "SeedNodes": ["akka.tcp://LargeNetworkSys@localhost:9221"]
    },
    "SerilogOptions": {
      "EnableSeq": false
    },
    "DispatcherConfig": "Defaults",
    "PersistenceOptions": {
      "Enabled": true
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
